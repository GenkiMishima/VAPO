D:float(480);
r:D/2;
E:float(6.85*10^4);
p:5;
Phi:1/16*sqrt(r/t);
AxGamma:1-0.901*(1-exp(-Phi));
AxSigma:0.6*AxGamma*E*t/r;
Sigma_ba:p*D/(2*t);
BeGamma:1-0.731*(1-exp(-Phi));
BeSigma:0.6*BeGamma*E*t/r;
Sigma_per:(5*p*D+6*p*t)/(10*t);
Sigma_bot:(p*(D+0.2*t))/(2*t);
Sigma_cir:(p*(D+0.4*t))/(4*t);
plot2d([AxSigma,BeSigma,Sigma_ba,Sigma_per,Sigma_bot,Sigma_cir],[t,0.2,10]);
t1:find_root(AxSigma=Sigma_ba,t,2.0,10);
t2:find_root(BeSigma=Sigma_ba,t,2.0,10);
t3:find_root(AxSigma=Sigma_per,t,2.0,10);
t4:find_root(BeSigma=Sigma_bot,t,2.0,10);
t5:find_root(AxSigma=Sigma_cir,t,2.0,10);
